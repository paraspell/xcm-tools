{
  "name": "@paraspell/xcm-router",
  "version": "8.8.0",
  "description": "Tool for XCM cross-chain asset exchanging across Polkadot and Kusama ecosystems",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "tsc --noEmit",
    "build": "rollup -c",
    "lint:check": "eslint .",
    "lint": "eslint --fix .",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts --sequence.concurrent",
    "runAll": "pnpm compile && pnpm format:write && pnpm lint && pnpm test",
    "updateAssets": "node --loader ts-node/esm --experimental-specifier-resolution=node ./scripts/updateAssets.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@acala-network/api": "^6.2.0",
    "@acala-network/app-util": "^4.1.8-14",
    "@acala-network/eth-providers": "^2.9.5",
    "@acala-network/sdk": "4.1.11",
    "@acala-network/sdk-core": "4.1.11",
    "@acala-network/sdk-swap": "4.1.11",
    "@acala-network/sdk-wallet": "4.1.8-14",
    "@crypto-dex-sdk/amm": "^0.1.4",
    "@crypto-dex-sdk/chain": "^0.1.4",
    "@crypto-dex-sdk/currency": "^0.1.4",
    "@crypto-dex-sdk/format": "^0.1.4",
    "@crypto-dex-sdk/math": "^0.1.4",
    "@crypto-dex-sdk/parachains-bifrost": "^0.1.4",
    "@crypto-dex-sdk/token-lists": "^0.1.4",
    "@galacticcouncil/sdk": "^5.6.0",
    "@paraspell/sdk-pjs": "workspace:*",
    "@polkadot/api": "^15.7.1",
    "@polkadot/api-base": "^15.7.1",
    "@polkadot/types": "^15.7.1",
    "@polkadot/types-codec": "^15.7.1",
    "@polkadot/util": "^13.4.3",
    "@zenlink-types/bifrost": "^1.7.7",
    "axios": "^1.8.2",
    "bignumber.js": "^9.1.2",
    "ethers-v6": "npm:ethers@^6.13.5",
    "ethers": "^5.7.0",
    "inter-exchange": "^2.0.0",
    "jsbi": "^4.3.0",
    "lodash-es": "^4.17.21",
    "postcss": "^8.5.3",
    "postinstall-postinstall": "^2.1.0",
    "react": "^19.0.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/plugin-syntax-import-attributes": "^7.26.0",
    "@babel/preset-env": "^7.26.9",
    "@codecov/rollup-plugin": "^1.9.0",
    "@galacticcouncil/api-augment": "^0.3.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/lodash-es": "^4.17.12",
    "@vitest/coverage-v8": "^3.0.7",
    "dotenv": "^16.4.7",
    "prettier": "^3.5.2",
    "rollup": "^4.34.8",
    "rollup-plugin-dts": "^6.1.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1"
  }
}
